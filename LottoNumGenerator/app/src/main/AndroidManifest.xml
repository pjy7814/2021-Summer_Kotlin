<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lottonumgenerator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/kr_lotto"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/kr_lotto"
        android:supportsRtl="true"
        android:theme="@style/Theme.LottoNumGenerator">
        <!-- AppTheme.Constellation 테마지정 -->
        <activity
            android:name=".ConstellationActivity"
            android:theme="@style/AppTheme.Constellation" />
        <!-- AppTheme.Name 테마지정 -->
        <activity
            android:name=".NameActivity"
            android:theme="@style/AppTheme.Name" />
        <!-- AppTheme.Result 테마지정 -->
        <activity
            android:name=".ResultActivity"
            android:theme="@style/AppTheme.Result" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>



</manifest>